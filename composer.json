{
    "name": "mikha-dev/laravel-translation-loader",
    "description": "Store your language lines in the database, yaml or other sources",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-translation-loader",
        "translate",
        "database",
        "db",
        "language",
        "i8n"
    ],
    "homepage": "https://github.com/mikha-dev/laravel-translation-loader",
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.1",
        "illuminate/translation": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TranslationLoader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\TranslationLoader\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\TranslationLoader\\TranslationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
